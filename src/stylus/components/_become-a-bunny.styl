.c-become
  clearfix()
  max-width 1024px
  margin 0 auto 100px
  position relative
  padding 0 30px
  &__header
    clearfix()
    position relative
    padding 25px 0 75px
    &--text
      width 70%
      text-align left
      float left
      p
        color black
        font-size 16px
        line-height 2em
    &--img
      width 30%
      float right
      position absolute
      bottom 0
      right 0
      max-width 330px
      img
        display block
        width 100%
  &__detail
    padding-left 25px
    clearfix()
    &--title
      width 100%
      float left
      padding 0px 0px 0px 15px
      position relative
      top -50px
      h2
        color black
        max-width 40%
        font-family $font-kh-68
        font-size 72px
        line-height .8em
        text-align left
        text-transform uppercase
        margin-top 0
        margin-bottom 0
      .top-line
        clearfix()
        hr
          margin 20px 0
          float left
          width 20px
          border 1px solid black
    &--img
      width calc(50% + 20px)
      overflow hidden
      img
        display block
        width 100%
      .bg--shadow
      img,
      .bg--shadow
        transition top 1s ease, opacity 1s ease, z-index 1s ease, transform ease 1s 
      &:hover
        .bg--shadow
          opacity 0
          z-index -1
        img
          transform scale(1.1,1.1)
    &--black
      width calc(50% + 20px)
      background-color black
      padding 20px 0
      margin-top 20px
      .top-line
        clearfix()
        hr
          float left
          margin-top 10px
          margin-bottom 0
          width 20px
          border 1px solid $col-gold
      p
        font-size 16px
        text-align left
        color white
        line-height 2em
        margin-bottom 0
        

    &--right
      clearfix()
      position relative
      .c-become__detail
        &--img
          position absolute
          float right
          right 0
          z-index 2
          top 0
        &--black
          float left
          position relative
          top -50px
          .top-line
            hr
              margin-left 50px
          p
            padding 20px 50px

    &--left
      clearfix()
      position relative
      .c-become__detail
        &--img
          float left
          position relative
          z-index 2
        &--black
          width 50%
          margin-top 0px
          float right
          position absolute
          right 0
          top 40px
          .top-line
            hr
              margin-left 50px
          p
            padding 20px 50px
.become-a-bunny
  overflow hidden
  margin-bottom -80px
@media $medium
  .c-become
    &__header
      &--text
        line-height 2em
    &__detail
      padding-left 10px
      &--img
        width calc(50% + 5px)
      &--title
        h2
          font-size 48px
      &--black
        margin-top 0
        padding 15px
      &--right
        .c-become__detail
          &--black
            p
              padding 10px 50px 10px 20px
      &--left
        .c-become__detail
          &--black
            p
              padding 10px 20px 10px 50px

@media $small
  .c-become
    padding 0
    &__header
      padding-bottom 25px
      &--text
        width calc(100% - 60px)
        line-height 2em
        text-align center
        padding 0 30px
      &--img
        display none
    &__detail
      padding-left 0
      &--title
        position static
        padding-left 0
        float none
        padding-bottom 25px
        h2
          font-size 36px
          max-width none
          text-align center
        .top-line
          hr
            float none
            margin 15px auto
      &--img
        width 100%
      &--black
        width 85%
        margin 0 auto
        padding 0
        margin-bottom 25px
      &--right
        .c-become__detail
          &--img
            float none
            position static
          &--black
            float none
            position relative
            top -20px
            padding 0
            p
              line-height 1.5em
              padding 0 25px 25px
              text-align center
            .top-line
              hr
                float none
                margin 20px auto
      &--left
        .c-become__detail
          &--img
            float none
            position static
          &--black
            width 85%
            float none
            position relative
            top -20px
            right initial
            p
              line-height 1.5em
              padding 0 25px 25px
              text-align center
            .top-line
              hr
                float none
                margin 20px auto
      
html:lang(zh-CN)
  .c-become
    &__detail
      &--title
        h2
          font-size 54px
          font-family $font-zh
          line-height 1em



@media $medium
  html:lang(zh-CN)
    .c-become
      &__detail
        &--title
          h2
            font-size 48px


@media $small
  html:lang(zh-CN)
    .c-become
      &__detail
        &--title
          h2
            font-size 40px
    
          
.c-result__popup
  display none
  position fixed
  width 100%
  height 100%
  top 0
  left 0
  background-color rgba(0,0,0,.7)
  z-index 201
  &.is-active
    display block
  &--loading,
  &--success,
  &--error
    display none
    width 300px
    height 300px
    position fixed
    top 50%
    margin-top -150px
    left 50%
    margin-left -150px
    background-repeat no-repeat
    background-size cover
    &.is-active
      display block
  &--loading
    background-image url("data:image/gif;base64,")
  &--success
    background-image url("data:image/jpg;base64,")
  &--error
    background-image url("data:image/jpg;base64,")


