.c-gallery__detail
  max-width 1024px
  padding 0 30px
  margin 0 auto
  .c-gallery
    &__top
      margin-top 15px
      display block
      &--img
        display block
        width 66%
        position relative
        overflow hidden
        .bg--shadow__text
          background-image url("data:image/png;base64,")
          background-repeat no-repeat
          background-size cover
        .img-background,
        .bg--shadow
          transition all 1s
        &:hover
          .bg--shadow
            opacity 0
            z-index -1
          .img-background
            transform scale(1.1,1.1)
          .img-background,
          .bg--shadow
            transition all 1s
        .c-shadow__text
          position absolute
          bottom 0
          padding 0 10px 8px
          text-align left
          width calc(100% - 20px)
          z-index 3
          .top-line
            clearfix()
            hr
              float left
              width 40px
              border 1px solid white
              margin-bottom 15px
              margin-top 0
          p
            clearfix()
            text-transform uppercase
            color white
            font-size 60px
            font-family $font-kh-27
            line-height 1em
            margin-bottom 0
            span
              font-family $font-bc
              color $col-gold
              display inline-block
              float right
              font-size 18px
              font-style italic
              text-transform capitalize
              padding-right 10px
        .img-background
          height 21.5vw
          max-height 256px
          background-size cover
          background-position 50%
          background-repeat no-repeat
    &__imglist
      clearfix()
    &__img
      width 32%
      float left
      margin-right 2%
      margin-bottom 15px
      display block
      position relative
      overflow hidden
      .img-background,
      .bg--shadow
        transition all 1s
      .bg--shadow__text
        background-image url("data:image/png;base64,")
        background-repeat no-repeat
        background-size cover
      &:hover
        .bg--shadow
          opacity 0
          z-index -1
        .img-background
          transform scale(1.1,1.1)
        .img-background,
        .bg--shadow
          transition all 1s
      .c-shadow__text
        position absolute
        bottom 0
        padding 0 10px 7px 13px
        text-align left
        width calc(100% - 20px)
        z-index 3
        .top-line
          clearfix()
          hr
            float left
            width 25px
            border 1px solid white
            margin-bottom 10px
            margin-top 0
        p
          clearfix()
          text-transform uppercase
          color white
          font-size 38px
          font-family $font-kh-27
          line-height 1em
          margin-bottom 0
          span
            font-family $font-bc
            color $col-gold
            display inline-block
            float right
            font-size 18px
            font-style italic
            text-transform capitalize
            padding-right 10px
      .img-background
        height 21.5vw
        max-height 256px
        background-size cover
        background-position 50%
        background-repeat no-repeat
    &__img:nth-child(3n)
      margin-right 0
    &__link
      font-family $font-kh-27
      font-size 40px
      display inline-block
      margin-top 40px
      color black
      text-transform uppercase
      line-height 1em
      &:hover
        color $col-gold
        text-decoration underline
      span
        display inline-block
        width 20px
        &:before
          position relative
          top 4px
          left -16px
          
  .owl
    &-carousel
      margin-top 15px
    &-pagination
      margin-top 15px
    &-theme
      .owl-controls
        .owl-page
          background-color white
          border-radius 0
          font-family $font-kh-68
          position relative
          margin 0 10px
          font-size 18px
          transition all .5s
          .owl-numbers
            border-radius 0
            background-color white
            color black
            opacity 1
            font-size 18px
            transition all .5s
            &:hover
              color $col-gold
          &.active
            background-color black
            border none
            .owl-numbers
              background-color black
              color white
              &:hover
                color white
          &:before
            content '・'
            position absolute
            right -20px
            top 8px
            color black
          &:last-child:before
            content ''
  
@media $medium
  .c-gallery__detail
    .c-gallery
      &__top--img
        .c-shadow__text
          padding-bottom 10px
          p
            font-size 48px
      &__img
        .c-shadow__text
          padding 0 10px 7px
          .top-line
            hr
              margin-bottom 10px
              width 20px
          p
            font-size 24px
@media $small
  .c-gallery__detail
    padding 0 10px
    .c-gallery
      &__top
        margin-top 7px
        img
          width 100%
        &--img
          width 100%
          .c-shadow__text
            padding-bottom 7px
            .top-line
              hr
                width 20px
            p
              font-size 40px
          .img-background
            height 40vw
      &__img
        .c-shadow__text
          padding 0 10px 5px 7px
          .top-line
            hr
              width 15px
          p
            font-size 22px
        .img-background
          height 40vw
      &__img:nth-child(1n)
        width 49%
        margin-right 2%
        margin-bottom 7px
      &__img:nth-child(2n)
        width 49%
        margin-right 0
        margin-bottom 7px
      &__link
        font-size 32px
        span:before
          left -12px
          top 2px
    .owl
      &-carousel
        margin-top 7px
      &-pagination
        margin-top 7px
    

html:lang(zh-CN)
  .c-gallery__detail
    .c-gallery
      &__top
        &--img
          .c-shadow__text
            p
              font-size 30px
              padding-bottom 7px
              font-family $font-zh
              span
                font-family $font-zh
      &__img
        .c-shadow__text
          p
            font-size 24px
            padding-bottom 5px
            font-family $font-zh
            span
              font-family $font-zh
      &__link
        font-family $font-zh
        font-size 30px

@media $medium
  html:lang(zh-CN)
    .c-gallery__detail
      .c-gallery
        &__top
          &--img
            .c-shadow__text
              p
                font-size 24px
        &__img
          .c-shadow__text
            p
              font-size 18px

@media $small
  html:lang(zh-CN)
    .c-gallery__detail
      .c-gallery
        &__top
          &--img
            .c-shadow__text
              p
                font-size 16px
                span
                  font-size 16px
        &__img
          .c-shadow__text
            p
              font-size 14px
              span
                font-size 14px
        &__link
          font-size 24px




