.c-join
  margin-top 125px
  clearfix()
  &__title
    width 20%
    float left
    .top-line
      clearfix()
      hr
        margin-top 0
        margin-bottom 0
        float left
        width 30px
        border 1px solid black
    h2
      color black
      font-family $font-kh-68
      font-size 72px
      line-height 1.5em
      text-align left
      text-transform uppercase
      margin-top 0
      margin-bottom 0
  &__form
    clearfix()
    width 65%
    float left
    background-color black
    padding 10px 10px 30px
    position relative
    box-sizing border-box !important
    form
      clearfix()
    *
      box-sizing border-box !important
    &--img
      width 100%
      clearfix()
      .form-field
        width 125px
        height 125px
        float left
        background-image url("data:image/png;base64,")
        background-size cover
        background-repeat no-repeat
        background-position 50%
      .c-join__form--update
        p
          padding-left 3px
          position absolute
          bottom 0
          left 0
          display block
          width 100%
          font-family $font-kh-26
          color black
          padding-left 22px
          padding-top 0px
          line-height 23px 
          font-size 22px
          text-align left
          background-image url("data:image/png;base64,")
          background-repeat no-repeat
          background-size 14px
          background-position 3px 1px
        input
          padding 0 !important
          height 100% !important
        .error-text
          width 100%
          text-align left
          left 0
          top 100%
          p
            background-image none
            font-family Arial, sans-serif
            font-size 12px
            line-height 1.5em
            padding-left 0
            position static
    &--half
      width 100%
      padding-right 100px
      clearfix()
      .form-field
        width calc(50% - 20px)
        float left
        +ie(9)
          width 46%
    &--full
      width 100%
      padding-right 100px
      clearfix()
      .form-field
        width calc(100% - 20px)
        float left
      .c-join__form--update
        p
          display block
          width 100%
          font-family $font-kh-27
          color white
          padding-left 35px
          padding-top 3px
          line-height 35px 
          font-size 30px
          text-align left
          background-color #373737
          background-image url("data:image/png;base64,")
          background-repeat no-repeat
          background-size 18px
          background-position 10px 10px
          word-break break-word
        input
          padding 0 !important
          height 100% !important
          z-index 3
        .file-path
          display none
      > p
        text-align left
        font-size 14px
        color white
        display inline-block
        width 100%
        padding 10px 0 15px 10px
    &--update
      position relative
      cursor pointer
      input
        position absolute
        top 0
        left 0
        opacity 0
        cursor pointer
      p
        width 100%
        
        
    .form-field
      background-color black
      margin 17.5px 10px
      position relative
      input
        width 100%
        height 36px
        border none
        background-color #373737
        font-family $font-kh-27
        padding-left 10px
        padding-top 3px
        font-size 30px
        color white
      textarea
        resize none
        display block
        width 100%
        background-color #373737
        font-family $font-kh-27
        padding-left 3px
        border none
        min-height 150px
        padding-left 10px
        padding-top 3px
        font-size 30px
        color white
        line-height 35px
      &.n-invalid
        border solid 1px #f00
        .error-text
          display block  
    
    .error-text
      display none  
      position absolute
      top 100%
      p
        text-align left
        color #f00
        font-size 12px
        &.error-zh
          display none
      
  &__img
    position absolute
    bottom 0
    right -227px
    width 52%
    min-width 345px
    img
      width 100%
      display block
  
  &__submit
    background-color white
    border none
    padding 5px
    line-height 1em
    font-size 30px
    font-family $font-kh-27
    color black
    float left
    margin-left 10px
    transition all 1s
    cursor pointer
    border-radius 0
    -webkit-appearance none
    &:hover
      background-color $col-gold
      color white
      text-decoration none
  
@media $medium
  .c-join
    margin-top 150px
@media $small
  .c-join
    margin-top 0
    &__title
      width 100%
      float none
      margin-bottom 40px
      h2
        font-size 36px
        text-align center
      .top-line
        hr
          float none
          margin 0 auto
    &__form
      width 100%
      float none
      padding 10px 30px 30px
      &--img
        margin auto
        width 270px
      &--half
        padding-right 0
        width 100%
        .form-field
          width calc(50% - 10px)
        &:nth-child(4)
          .form-field
            width calc(100% - 10px)
      &--full
        padding 0
        width 100%
        .form-field
          width calc(100% - 10px)
        .c-join__form--update
          p
            font-size 26px
      .form-field
        margin 17.5px 5px
    &__img
      display none
    &__submit
      float none
      
@media (min-width: 925px) and (max-width: 1167px)
  .c-join
    &__title
      width 210px
    &__form
      width calc(100% - 210px)
      &--half
        padding-right 0
      &--full
        padding-right 0
    &__img
      display none
@media (min-width: 751px) and (max-width: 924px)
  .c-join
    &__title
      width 100%
      padding-left 15px
      margin-bottom 20px
    &__form
      width 100%
      &--half
        padding-right 0
      &--full
        padding-right 0
    &__img
      display none
      
@media (max-width: 330px)
  .c-join__form
    padding 10px

html.ios.mobile
  .c-join__form--full
    .c-join__form--update
      display none

html:lang(zh-CN)
  .c-join
    &__title
      h2
        font-size 48px
        font-family $font-zh
        line-height 1em
        padding 15px 0
      
    &__form
      .form-field
        input
          font-size 20px
          font-family $font-zh
          line-height 36px
          padding 0
          padding-left 10px
        textarea
          font-size 20px
          font-family $font-zh
      .error-text
        display none  
        p
          &.error-en
            display none
          &.error-zh
            display block
      &--full
        .c-join__form--update
          p
            font-size 20px
            font-family $font-zh
      &--img
        .c-join__form--update
          p
            font-size 11px
            font-family $font-zh
            background-size 9px
            background-position 4px 4px
            
    &__submit
      font-size 20px
      font-family $font-zh
      line-height 25px
      padding 5px 10px
      font-weight bold

@media $small
  html:lang(zh-CN)
    .c-join
      &__title
        h2
          font-size 36px

@css{
  input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {
    color: white;
    font-size: 30px;
    font-family: 'Knockout-HTF27', Arial, sans-serif;
    line-height: 35px;
  }
  input:-moz-placeholder, textarea:-moz-placeholder {
    color: white;
    font-size: 30px;
    font-family: 'Knockout-HTF27', Arial, sans-serif;
    line-height: 35px;
  }
  input::-moz-placeholder, textarea::-moz-placeholder {
    color: white;
    font-size: 30px;
    font-family: 'Knockout-HTF27', Arial, sans-serif;
    line-height: 35px;
  }
  input:-ms-input-placeholder, textarea:-ms-input-placeholder {
    color: white;
    font-size: 30px;
    font-family: 'Knockout-HTF27', Arial, sans-serif;
    line-height: 35px;
  }
  @media (max-width: 750px) {
    input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {
      font-size: 26px;
    }
    input:-moz-placeholder, textarea:-moz-placeholder {
      font-size: 26px;
    }
    input::-moz-placeholder, textarea::-moz-placeholder {
      font-size: 26px;
    }
    input:-ms-input-placeholder, textarea:-ms-input-placeholder {
      font-size: 26px;
    }
  }
  html:lang(zh-CN) input::-webkit-input-placeholder, html:lang(zh-CN) textarea::-webkit-input-placeholder {
    font-size: 20px;
    font-family: 'SimHei', Arial, sans-serif;
    line-height: 35px;
  }
  html:lang(zh-CN) input:-moz-placeholder, html:lang(zh-CN) textarea:-moz-placeholder {
    font-size: 20px;
    font-family: 'SimHei', Arial, sans-serif;
    line-height: 35px;
  }
  html:lang(zh-CN) input::-moz-placeholder, html:lang(zh-CN) textarea::-moz-placeholder {
    font-size: 20px;
    font-family: 'SimHei', Arial, sans-serif;
    line-height: 35px;
  }
  html:lang(zh-CN) input:-ms-input-placeholder, html:lang(zh-CN) textarea:-ms-input-placeholder {
    font-size: 20px;
    font-family: 'SimHei', Arial, sans-serif;
    line-height: 35px;
  }
  @media (max-width: 750px) {
    html:lang(zh-CN) input::-webkit-input-placeholder, html:lang(zh-CN) textarea::-webkit-input-placeholder {
      font-size: 18px;
    }
    html:lang(zh-CN) input:-moz-placeholder, html:lang(zh-CN) textarea:-moz-placeholder {
      font-size: 18px;
    }
    html:lang(zh-CN) input::-moz-placeholder, html:lang(zh-CN) textarea::-moz-placeholder {
      font-size: 18px;
    }
    html:lang(zh-CN) input:-ms-input-placeholder, html:lang(zh-CN) textarea:-ms-input-placeholder {
      font-size: 18px;
    }
  }
}



