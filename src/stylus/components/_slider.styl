.c-slider
  position relative
  img
    width 100%
    display block
  .owl
    &-pagination
      display inline-block
      position absolute
      bottom 20%
      left 20%
    &-theme
      .owl-controls
        .owl-buttons
          .owl-prev,
          .owl-next
            position absolute
            top 45%
            width 67px
            height 27px
            border-radius 0
            background-color transparent
            background-repeat no-repeat
            background-size cover
            opacity 1
            padding 0
          .owl-prev
            background-image url("data:image/png;base64,")
            left calc(125px + 1%)
          .owl-next
            background-image url("data:image/png;base64,")
            right 5%
            
  &__link
    position absolute
    right 0
    top 20px
    max-width 120px 
  &__item
    color black
    img.c-slider__item-mobile
      display none

@media $medium
  .c-slider
    .owl
      &-pagination
        left 20%
        bottom 15%
      &-theme
        .owl-controls
          .owl-buttons
            .owl-prev,
            .owl-next
              width 44px
              height 17px

@media $small
  .c-slider
    .owl
      &-carousel
        overflow hidden
      &-buttons
        display none
      &-pagination
        left 3%
        bottom 20%
    &__item
      img.c-slider__item-mobile
        display block
      img.c-slider__item-desktop
        display none
    &__link
      display none

@media (max-width:351px)
  .c-slider
    .owl-pagination
      bottom 6%
      
      


