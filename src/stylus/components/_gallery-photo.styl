.c-photo
  max-width 1024px
  padding 0 30px
  margin 50px auto 0
  &__blocks
    clearfix()
    .c-photo__block
      width calc(50% - 5px)
      float left
      margin-bottom 10px
      &:nth-child(odd)
        margin-right 10px
    
  &__back
    width 100%
    margin-bottom 10px
    clearfix()
    a
      font-family $font-kh-27
      font-size 24px
      line-height 1.5em
      text-transform uppercase
      color black
      display inline-block
      float left
      position relative
      padding-left 12px
      span
        position absolute
        width 10px
        top 6px
        left -12px
      &:hover
        color $col-gold
        text-decoration underline
  &__crosswise
    clearfix()
    .c-photo__img
      width 100%
      overflow hidden
      position relative
      cursor pointer 
      &:first-child
        margin-bottom 10px
      &--crosswise
        height calc(20vw - 5px)
        max-height 250px 
        background-size cover
        background-position 50% top
        background-repeat no-repeat
      .c-photo__img--crosswise,
      .bg--shadow
        transition all 1s
      .bg--shadow__text
        background-image url("data:image/png;base64,")
        background-repeat no-repeat
        background-size cover
      &:hover
        .bg--shadow
          opacity 0
          z-index -1
        .c-photo__img--crosswise
          transform scale(1.1,1.1)
  &__lengthways
    clearfix()
    .c-photo__img
      float left
      width calc(50% - 5px)
      overflow hidden
      position relative
      cursor pointer
      &:first-child
        margin-right 10px
      &--lengthways
        height 40vw
        max-height 510px
        background-size cover
        background-position 50%
        background-repeat no-repeat
      .c-photo__img--lengthways,
      .bg--shadow
        transition all 1s
      .bg--shadow__text
        background-image url("data:image/png;base64,")
        background-repeat no-repeat
        background-size cover
      &:hover
        .bg--shadow
          opacity 0
          z-index -1
        .c-photo__img--lengthways
          transform scale(1.1,1.1)
  .c-shadow__text
    position absolute
    bottom 0
    padding 0 10px 8px
    text-align left
    z-index 3
    p
      text-transform uppercase
      color white
      font-size 30px
      font-family $font-kh-27
      line-height 1em   
      margin-bottom 0
  

  .owl  
    &-pagination
      margin-top 15px
    &-theme
      .owl-controls
        .owl-page
          background-color white
          border-radius 0
          font-family $font-kh-68
          position relative
          margin 0 10px
          font-size 18px
          transition all .5s
          .owl-numbers
            border-radius 0
            background-color white
            color black
            opacity 1
            font-size 18px
            transition all .5s
            &:hover
              color $col-gold
          &.active
            background-color black
            border none
            .owl-numbers
              background-color black
              color white
              &:hover
                color white
          &:before
            content 'ãƒ»'
            position absolute
            right -20px
            top 8px
            color black
          &:last-child:before
            content ''
    

@media $medium
  .c-photo
    .c-shadow__text
      padding 0 10px 7px
      p
        font-size 24px
@media $small
  .c-photo
    padding 0
    margin-top 50px
    &__blocks
      .c-photo__block
        width 100%
        float left
        margin-bottom 6px
        &:nth-child(odd)
          margin-right 0
    &__crosswise
      .c-photo__img
        &:first-child
          margin-bottom 6px
        &--crosswise
          height 50vw
    
    &__lengthways
      .c-photo__img
        width calc(50% - 3px)
        &:first-child
          margin-right 6px
        &--lengthways
          height 90vw
    
    .c-shadow__text
      padding 0 10px 5px 7px
      p
        font-size 24px

// gallery photo popup
.c-photo__popup
  position fixed
  width 100%
  height 100%
  top 0
  left 0
  background-color rgba(0,0,0,.9)
  z-index 201
  overflow auto
  display none
  &.is-active
    display block
  &--slider
    width 100%
    max-width 500px
    margin 7% auto 0
    position relative
  &--block
    img
      width 100%
      display block
    .img-mobile
      display none
  &--text
    background-color black
    h2
      padding 15px 0
      top -25px
      font-size 60px
      font-family $font-kh-27
      text-transform uppercase
      color white
      line-height 1em
      transition color .8s
      margin-top 0
      margin-bottom 0
    .top-line
      hr
        width 35px
        margin 0px auto
        border 1px solid $col-gold
    > p
      font-size 16px
      line-height 2em
      color white
      padding 20px 30px 30px 
      margin-bottom 0
  &--close
    cursor pointer
    position absolute
    right -50px
    top 0px
    width 35px
    height 35px
    color white
    background-color black
    span
      font-size 20px
      position relative
      top 7px
  .owl
    &-pagination
      display none
    &-prev
      position absolute
      top 30%
      left 25px
      line-height 18px
      &:before
        content "\e900"
        font-size 30px
        position relative
        top 10px
        left -3px
        color white
      &:hover
        &:before
          color black
    &-next
      position absolute
      top 30%
      right 25px
      line-height 18px
      &:before
        content "\e905"
        font-size 30px
        position relative
        top 10px
        left -3px
        color white
      &:hover
        &:before
          color black
    &-theme
      .owl-controls
        .owl-buttons
          div
            background-color black
            width 38px
            height 38px
            border-radius 0
            opacity 1
            padding 0
            overflow hidden
            font-family 'icomoon' !important
            transition background-color .8s, color .8s
            &:hover
              background-color $col-gold
@media $small
  .c-photo__popup
    &--slider
      margin-top 55px
    &--block
      .img-desktop
        display none
      .img-mobile
        display block   
    &--text
      h2
        font-size 48px
        top -17px
      .top-line
        hr
          width 25px
      > p
        line-height 1.5em
        padding 20px 40px 30px
    &--close
      width 45px
      height 45px
      top -55px
      right 0
      background-color $col-gold
      span
        color black
        top 10px
        font-size 24px 
    .owl
      &-prev
        top 30vw
        left 0
        &:before
          top 17px
          left 0px
          color black
      &-next
        top 30vw
        right 0
        &:before
          top 17px
          left 0px
          color black
        
      &-theme
        .owl-controls
          .owl-buttons
            div
              background-color $col-gold
              width 50px
              height 50px

html:lang(zh-CN)
  .c-photo
    &__back
      a
        font-size 20px
        font-family $font-zh
        span
          top 3px
          font-size 24px
    .c-shadow__text
      p
        font-family $font-zh
        font-size  20px
        padding-bottom 5px
  .c-photo__popup
    &--text
      h2
        top -20px
        font-size 40px
        font-family $font-zh
@media $medium
  html:lang(zh-CN)
    .c-photo__popup
      &--text
        h2
          font-size 36px
      .c-shadow__text
        p
          font-size  18px
@media $small
  html:lang(zh-CN)
    .c-photo__popup
      &--text
        h2
          font-size 24px
      .c-shadow__text
        p
          font-size  16px
