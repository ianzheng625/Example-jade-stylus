.c-gallery
  max-width 1024px
  margin 20px auto 50px
  background-color black
  width 100%
  .button--learn-more
    color black
    background-color white
    &:hover
      color white
      background-color $col-gold
  img
    width 100%
    display block
  &__small
    width 25%
    float left
    border-left 4px solid white
    &-square:first-child
      border-bottom 4px solid white
      display block
    &-square
      display block
      position relative
      overflow hidden
      .img-background
        height 21.5vw
        max-height 256px
        background-size cover
        background-position 50%
        background-repeat no-repeat
        +ie(9)
          min-height 100px
      .bg--shadow__text
        background-image url("data:image/png;base64,")
        background-repeat no-repeat
        background-size cover
      &:hover
        .bg--shadow
          opacity 0
          z-index -1
        .img-background
          transform scale(1.1,1.1)
      .img-background,
      .bg--shadow
        transition top 1s ease, opacity 1s ease, z-index 1s ease, transform ease 1s  
  &__big
    width calc(42% - 10px)
    float left
    padding 4% 4% 0
    text-align left
    .top-line
      clearfix()
      hr
        margin-top 0
        margin-bottom 0
        border 1px solid white
        width 20px
        float left
    h2
      font-size 88px
      color white
      line-height 1.2em
      font-family $font-kh-68
      text-transform uppercase
      margin-top 0
      margin-bottom 0
    h3
      font-family $font-bc
      font-size 32px
      line-height 1em
      color white 
      margin-top 20px
      margin-bottom 0
      font-style italic
      text-transform uppercase
    &-mobile
      display none
      border-bottom 4px solid white
      padding 20px 0 40px
      h2
        font-size 88px
        color white
        line-height 1.2em
        font-family $font-kh-68
        text-transform uppercase
        margin-top 0
        margin-bottom 0
      h3
        font-family $font-bc
        font-size 48px
        line-height 1em
        color white 
        margin-top 20px
        margin-bottom 0
        font-style italic
      .top-line
        hr
          border 1px solid white
          width 20px
          margin 0 auto
  &__rectangle
    width calc(25% + 2px)
    float left
    border-right 4px solid white
    overflow hidden
    position relative
    .img-background
      height 43.48vw
      max-height 516px
      background-size cover
      background-position 50%
      background-repeat no-repeat
      +ie(9)
        min-height 200px
    .bg--shadow__text
      background-image url("data:image/png;base64,")
      background-repeat no-repeat
      background-size cover
    &:hover
      .bg--shadow
        opacity 0
        z-index -1
      .img-background
        transform scale(1.1,1.1)
    .img-background,
    .bg--shadow
      transition top 1s ease, opacity 1s ease, z-index 1s ease, transform ease 1s  
  .button--learn-more
    margin 15px 0 0  
  .c-shadow__text
    position absolute
    bottom 0
    padding 0 10px 8px
    text-align left
    width calc(100% - 20px)
    z-index 3
    .top-line
      clearfix()
      hr
        float left
        width 25px
        border 1px solid white
        margin-bottom 10px
        margin-top 0
    p
      text-transform uppercase
      color white
      font-size 38px
      font-family $font-kh-27
      line-height 1em
      margin-bottom 0
      clearfix()
    span
      font-family $font-bc
      color $col-gold
      display inline-block
      font-size 18px
      font-style italic
      text-transform capitalize
      margin-bottom 10px

@media $medium
  .c-gallery
    &__big
      width calc(44% - 10px)
      padding 3% 3% 0
      h2
        font-size 72px
        line-height 1em
      h3
        margin-top 10px
        font-size 22px
    .c-shadow__text
      .top-line
        hr
          width 20px
          margin-bottom 7px
      p
        font-size 32px
@media $small
  .c-gallery
    margin 4px
    width calc(100% - 8px)
    &__small
      width calc(50% - 3px)
      border-left 2px solid white
      &-square
        .img-background
          height 50vw
          max-height 372.5px
          +ie(9)
            min-height 100px
    &__big
      display none
      &-mobile
        display block
        h2
          font-size 60px
        h3
          font-size 20px
          padding 0 20px 20px
    &__rectangle
      width calc(50% - 1px)
      border-right 2px solid white
      .img-background
        height calc(100vw + 4px)
        max-height none
        +ie(9)
          min-height 200px
    .button--learn-more
      color black
      background-color $col-gold
    .c-shadow__text
      .top-line
      p
        font-size 30px

html:lang(zh-CN)
  .c-gallery
    &__big
      h2
        font-weight bold
        line-height 1em
        font-family $font-zh
        font-size 60px
        padding 15px 0
      h3
        font-family $font-zh
        font-size 28px
        line-height 1.2em
      &-mobile
        h2
          font-weight bold
          font-size 40px
          font-family $font-zh
          padding 15px 0
        h3
          font-family $font-zh
          font-size 20px
    .c-shadow__text
      p
        font-family $font-zh
        font-size 23px
      span
        font-family $font-zh
        font-size 16px
@media $medium
  html:lang(zh-CN)
    .c-gallery
      &__big
        h2
          font-size 54px
        h3
          font-size 24px
      .c-shadow__text
        p
          font-size 20px
          span
            font-size 16px
@media $small
  html:lang(zh-CN)
    .c-gallery
      &__big
        h2
          font-size 40px
        h3
          font-size 20px
      .c-shadow__text
        p
          font-size 18px
          span
            font-size 16px
